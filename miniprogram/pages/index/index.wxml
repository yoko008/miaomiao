<view class='container'>
  <view class='top-button-view'>
    <button class='top-button zhichu-button'>支出</button>
    <button class='top-button shouru-button'>收入</button>
  </view>
  <view class='text-view'>
    <input class='text-input beizhu-input' type='text' placeholder='备注'>
    </input>
    <input class='text-input jine-input' type='digit' placeholder='金额'>
    </input>
  </view>

<view class="section">
  <picker mode="multiSelector" bindchange="bindMultiPickerChange" bindcolumnchange="bindMultiPickerColumnChange" value="{{multiIndex}}" range="{{multiArray}}">
    <view class="picker">
     {{multiArray[0][multiIndex[0]]}}-{{multiArray[1][multiIndex[1]]}}
    </view>
  </picker>
</view>
<view class='datetime-view'>
<view class="section">
  <picker mode="date" value="{{date}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
    <view class="picker">
     {{date}}
    </view>
  </picker>
</view>
<view class="section">
  <picker mode="time" value="{{time}}" start="09:01" end="21:01" bindchange="bindTimeChange">
    <view class="picker">
       {{time}}
    </view>
  </picker>
</view>
</view>




